{
    "version": 2,
    "name": "library-management-frontend",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "cache-control": "s-maxage=31536000,immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "REACT_APP_API_URL": "https://librarymanagementsystem-a5fi.onrender.com"
    }
}